                "ДНІВ_У_РОЦІ = 365\n",
                "ГОДИН_У_ДНІ = 24\n",
                "ХВИЛИН_У_ГОДИНІ = 60\n",
                "ХВИЛИН_У_РОЦІ = ХВИЛИН_У_ГОДИНІ * ГОДИН_У_ДНІ * ДНІВ_У_РОЦІ\n",
                "\n",
                "хвилини = float(input('Введіть кількість хвилин: '))\n",
                "роки = хвилини / ХВИЛИН_У_РОЦІ\n",
                "print(str(хвилини) + ' хвилин це приблизно ' + str(round(роки, 3)) + ' років.')"
            ]
