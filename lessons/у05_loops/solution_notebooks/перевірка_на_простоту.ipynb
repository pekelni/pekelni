            "source": [
                "число = int(input('Введіть ціле додатнє число: '))\n",
                "результат_перевірки = 'просте'\n",
                "for i in range(2, число):\n",
                "    if число % i == 0:\n",
                "        print(f'Ділиться на {i}')\n",
                "        результат_перевірки = 'не просте'\n",
                "        break\n",
                "print(результат_перевірки)"
            ]
        }
