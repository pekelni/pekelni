            "source": [
                "число = int(input('Введіть число: '))\n",
                "сума_цифр = 0\n",
                "лічильник = 0\n",
                "\n",
                "while число:\n",
                "    цифра = число % 10\n",
                "    сума_цифр += цифра\n",
                "    лічильник += 1\n",
                "    print(f'Сума перших {лічильник} цифр: {сума_цифр}')\n",
                "    число //= 10\n",
                "\n",
                "print(f'Фінальна сума цифр: {сума_цифр}')"
            ]
        }
    ],
